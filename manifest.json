{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "gamepad.js" ],
      "matches": [ "https://html5gamepad.com/*", "https://hardwaretester.com/*", "https://www.xbox.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Maps keyboard/mouse to an emulated gamepad",
   "icons": {
      "128": "img/ts-128.png",
      "16": "img/ts-16.png",
      "32": "img/ts-32.png",
      "48": "img/ts-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Keyboard/Mouse Emulated Gamepad",
   "page_action": {
      "default_icon": {
         "128": "img/ts-128.png",
         "16": "img/ts-16.png",
         "32": "img/ts-32.png",
         "48": "img/ts-48.png"
      },
      "default_popup": "settings.html"
   },
   "permissions": [ "storage", "declarativeContent" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.25",
   "web_accessible_resources": [ "img/controls/*.svg", "res/*.json" ]
}
